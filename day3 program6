Search an Element



#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,p=0;
    scanf("%d",&n);
    
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    int x;
    scanf("%d",&x);
    
    for(int i=0;i<n;i++)
    {
        if(x==a[i])
        {
            p=1;
        }
    }
    if(p==1)
        printf("%d is presented in an array.",x);
     else
        printf("%d is not presented in an array.",x);
    return 0;
}
