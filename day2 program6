#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    
    if((a>b) && (a>c))
    {
        printf("Number1 is maximum with value of %.1f",(float)a);
    }
    else if((b>a) && (b>c))
    {
        printf("Number2 is maximum with value of %.1f",(float)b);
    }
    else if((c>=a) && (c>=b))
    {
        printf("Number3 is maximum with value of %.1f",(float)c);
    }
    return 0;
}
