Second minimum element in an array


#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int n;
    scanf("%d",&n);
    
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
        if(a[i]>a[j])
        {
            int b=a[i];
            a[i]=a[j];
            a[j]=b;
        }
        }
            
    }
    
    printf("%d",a[1]);
    return 0;
}
