#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char a[10];
    int b,c,d,e,f;
    
    scanf("%s %d %d %d %d %d",a,&b,&c,&d,&e,&f);
    
    printf("Name of the Student:%s\n",a);
    printf("Total marks:%d\n",(b+c+d+e+f));
    printf("Average marks:%.1f",(float)(b+c+d+e+f)/5);
    return 0;
}
