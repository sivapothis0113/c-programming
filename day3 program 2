PRODUCT OF LIST

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,x=1;
    scanf("%d",&n);
    
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        x=x*a[i];
    }
    printf("The Product of list value is %.2f",(float)x);
    return 0;
}
